
public class central {
	private String DVLServer = null;
	private String KKLServer = null;
	private String WSTServer = null;
	
	public void setDVLServer(String inUrl)
	{
		DVLServer = inUrl;
	}
	
	public void setKKLServer(String inUrl)
	{
		KKLServer = inUrl;
	}
	
	public void setWSTServer(String inUrl)
	{
		WSTServer = inUrl;
	}
	
	public String getDVLServer(String inUrl)
	{
		return DVLServer;
	}
	
	public String getKKLServer(String inUrl)
	{
		return KKLServer;
	}
	
	public String getWSTServer(String inUrl)
	{
		return WSTServer;
	}
	
}
