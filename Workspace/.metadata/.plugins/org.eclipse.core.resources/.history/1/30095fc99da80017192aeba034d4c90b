
public class central {
	String DVLServer = null;
	String KKLServer = null;
	String WSTServer = null;
	
	public void setDVLServer(String inUrl)
	{
		DVLServer = inUrl;
	}
	
	public void setKKLServer(String inUrl)
	{
		KKLServer = inUrl;
	}
	
	public void setWSTServer(String inUrl)
	{
		WSTServer = inUrl;
	}
	
	public void getDVLServer(String inUrl)
	{
		return DVLServer;
	}
	
	public void getKKLServer(String inUrl)
	{
		return KKLServer;
	}
	
	public void getWSTServer(String inUrl)
	{
		return WSTServer;
	}
}
